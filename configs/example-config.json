{
  "package_use": {
    "dev-lang/python:3.11": ["ssl", "threads", "sqlite", "readline"],
    "sys-devel/gcc": ["openmp", "fortran", "cxx"],
    "app-editors/vim": ["python", "ruby"],
    "dev-db/postgresql": ["ssl", "xml", "uuid"]
  },
  "package_keywords": {
    "dev-lang/python:3.11": ["~amd64"],
    "dev-util/cmake": ["~amd64"]
  },
  "package_mask": [
    ">=dev-lang/python-3.12",
    ">=sys-devel/gcc-14.0"
  ],
  "package_unmask": [],
  "make_conf": {
    "MAKEOPTS": "-j8",
    "EMERGE_DEFAULT_OPTS": "--quiet-build=y --jobs=4 --load-average=8",
    "FEATURES": "buildpkg parallel-install parallel-fetch",
    "ACCEPT_LICENSE": "*",
    "PORTAGE_NICENESS": "15"
  },
  "environment": {
    "FEATURES": "buildpkg",
    "PORTAGE_TMPDIR": "/var/tmp"
  },
  "global_use": [
    "systemd",
    "-consolekit",
    "ipv6",
    "unicode",
    "nls"
  ],
  "repos": [
    {
      "name": "gentoo",
      "location": "/var/db/repos/gentoo",
      "sync_type": "git",
      "sync_uri": "https://github.com/gentoo-mirror/gentoo.git",
      "priority": 100
    }
  ]
}
